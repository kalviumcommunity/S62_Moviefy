{
  "version": "1",
  "name": "CRUD Operations",
  "type": "collection",
  "requests": [
    {
      "name": "Get All Users",
      "type": "http",
      "seq": 1,
      "url": "http://localhost:8080/user",
      "method": "GET",
      "body": "none",
      "auth": "none"
    },
    {
      "name": "Create User",
      "type": "http",
      "seq": 2,
      "url": "http://localhost:8080/user",
      "method": "POST",
      "body": {
        "name": "John Doe",
        "email": "john@example.com",
        "password": "password123"
      },
      "auth": "none"
    },
    {
      "name": "Update User",
      "type": "http",
      "seq": 3,
      "url": "http://localhost:8080/user/:id",
      "method": "PUT",
      "body": {
        "name": "John Updated",
        "email": "john.updated@example.com",
        "password": "newpassword123"
      },
      "auth": "none"
    },
    {
      "name": "Delete User",
      "type": "http",
      "seq": 4,
      "url": "http://localhost:8080/user/:id",
      "method": "DELETE",
      "body": "none",
      "auth": "none"
    }
  ],
  "ignore": ["node_modules", ".git"]
}
